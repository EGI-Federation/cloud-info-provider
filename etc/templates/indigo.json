% for image_id, image in attributes["images"].iteritems():
{
  image_id: "${image_id}",
  % if "docker_id" in image:
  docker_id: "${image["docker_id"]}",
  % endif
  % if "docker_name" in image:
  docker_id: "${image["docker_name"]}",
  % endif
  % if "docker_tag" in image:
  docker_id: "${image["docker_tag"]}",
  % endif
  image_name: "${image["image_name"]}",
  image_version: "${image["image_version"]}",
  image_marketplace_id: "${image["image_marketplace_id"]}",
  image_description: "${image["image_description"]}"
},
% endfor
